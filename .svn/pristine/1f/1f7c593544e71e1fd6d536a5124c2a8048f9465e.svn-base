@charset "utf-8";
/* CSS Document */

/* reset */
body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,textarea,table,th,td,select,img,div,dl,dt,dd,input{ margin:0; padding:0; }
em { font-style:normal; }
img{ border:none;vertical-align:top;/*谨慎使用*/ }
li{ list-style:none; }
/*table { border-collapse:collapse; }/*表格在默认状态下，边框与边框之间会有一定的空隙，border-collapse:collapse;可以消除边框与边框之间的间距*/
input,select,textarea{ outline:none; }/*无轮廓*/
textarea { resize:none; overflow:auto; }
a{ text-decoration:none; color:black; }
a:hover{ color:#eb0e1f !important; }
body { width:100%; font-size:16px; font-family:"Microsoft Yahei"; }

.active{ color:#eb0e1f; }
.left{ float:left; }
.right{ float:right;}

#top,#footer,#botoom{ width:100%; margin:0 auto; }
#search,#nav,#content{ width:1200px; margin:0 auto; }

/*顶部*/
#top{
	height:30px;
	background:#f5f5f5;
	font-size:16px;
	line-height:30px; 
}
#top .text{
	width:1200px;
	height:30px;
	margin:0 auto;
}
#top li{
	float:left;
	padding-left:20px;
}
/*搜索*/
#search{
	height:80px;
	margin:50px auto;
}
#search form{
	display:inline-block;
	width:730px;
	height:48px;
	margin:6px 0 0 74px;
}
#search .text{
	width:586px;
	height:44px;
	border:2px solid #fa0909;
	padding:0 15px;
	font-size:18px;
	vertical-align:middle;
}
#search .submit{
	width:96px;
	height:48px;
	border:none;
	font-size:22px;
	font-family:"微软雅黑";
	color:#fff;
	background:red;
	vertical-align:middle;
}
#search ul{
	padding-left:240px;
}
#search li{
	float:left;
	padding-left:20px;
}
#search .hot{
	padding-left:240px;
}
#search .hot a{
	color:#9e9898;
	margin:0 20px;
}
/*导航*/
#nav{
	height:50px;
	font-size:20px;
	line-height:50px;
}
#nav .list{
	width:300px;
	height:50px;
	background:#eb0e1f;
	display:inline-block;
}
#nav p{
	padding-left:20px;
	color:#fff;
	line-height:50px;
	background:url(../images/index/nav_list.gif) 250px 15px  no-repeat;
}
#nav ul{
	width:900px;
	float:right;
}
#nav li{
	float:left;
	padding-left:40px;
}
/*中间内容*/
#content{
	width:100%;
	background:#f4f4f4;
}
#content .inquire_price{width:1200px; margin:0 auto;}
#content .main{
	width:1198px;
	height:650px;
	margin:0 auto;
	border-left:1px dashed #cdcdcd;
	border-right:1px dashed #cdcdcd;
	position:relative;
}
#content .main .nav{
	height:80px;
	font-size:20px;
	color:#4a4a4a;
	line-height:60px;
}
#content .main .picture{
	margin-left:20px;
		
}
#content .main .picture .source_picture{
	width:389px;
	height:379px;	
}
#content .main .picture ul li{
	display:inline-block;
	margin:20px 0 0 20px;
}
#content .main .picture ul a{
	display:inline-block;
	width:100px;
	height:100px;
	border:1px solid #a1a1a1;	
}
#content .main .picture ul a:hover,#content .main .picture ul .default{
		border:1px solid #ff2626;
}
#content .main .picture .collect{
	margin:20px 0 0 60px;
	
}
#content .main .picture .collect a{
	padding-left:40px;
	background:url(../images/details/collect.gif) no-repeat 0 0;
	font-size:22px;	
}
#content .main .picture .collect a:hover{
	background:url(../images/details/collected.gif) no-repeat 0 0;
}
#content .main .info{
	width:675px;
	height:350px;
	margin-right:80px;
	background:url(../images/details/brand_info_bg.jpg);
}
#content .main .info .top{
	margin:10px;	
}
#content .main .info .top p{
	color:#fff;
	line-height:50px;	
}
#content .main .info .details{
	margin-left:10px;
}
#content .main .info .details li{
	padding-left:10px;
	font-size:20px;
	line-height:45px;
}
#content .main .info .details span{
	color:#6a6a6a;	
}
#content .main .info .number{
	margin:20px 0 50px 350px;	
}
#content .main .info a{
	display:inline-block;
	font-size:30px;
	line-height:30px;
	margin-left:40px;
	padding:15px;
	background:#ffcece;
	color:#ff2524;
	border:1px solid #ff2524;
}
#content .main .info a:hover{
	color:#fff !important;
	background:#ff2524;
}
#content .others{
	width:1200px;
	height:3000px;
	padding:60px 0;
	margin:0 auto;
	background:#fff;	
}
#content .inquire_price{
	height:400px;
	text-align:center;
	background:#d5d5d5;
}
#content .inquire_price img{
	margin:20px 0;
}
#content .inquire_price p{
	width:1200px;
	height:40px;
	margin-bottom:20px;
	border-top:1px solid #8e8e8e;
	font-size:22px;
	line-height:40px;
	color:#ff2929;	
}
#content .inquire_price span{
	font-size:20px;
	line-height:40px;
}
#content .inquire_price input{
	width:288px;
	height:38px;
	padding-left:10px;
	font-size:20px;
	line-height:40px;
	background:#fff;
	border:1px solid #ff2626;
}
#content .inquire_price .default input{
		background:#b9b9b9;
		text-align:center;
		color:#ff2121;
}
#content .inquire_price a{
	font-size:26px;
	color:#fff;
	background:#ff3737;
	padding:5px 40px;
}
#content .inquire_price a:hover{
	color:#000 !important;	
}





.content_middle{
	position: absolute;
	top:0;
}
/*---move--*/

.content_middle_left{
	float: left;
	width: 300px;
	height: 500px;
}

.content_info li {
    float: left;
	height: 63px;
	width: 300px;
	border-bottom: 1px solid white;
}
.content_infos{
	width: 300px;
	height: 25px;
}
.content_infos li{
	height: 25px;
	line-height: 25px;
	float: left;
	width: 60px;
	text-align: center;
	border-bottom: none;
}
.content_infos li{
	height: 25px;
	line-height: 25px;
	width: 60px;
	text-align: center;
}
.content_middle_bottom{
    width: 300px;
	height: 51px;
	background-color: red;
}
.content_middle_top{
    height: 449px;
    width: 300px;
	background-color: rgba(60,60,60,0.9); /* 其他浏览器 */	
}
.content_info { 
    list-style-type: none;
    width: 300px;
    height: 449px;
}
.content_infos a{
	color: #8b8b8b;
}
.content_info li h2{
	width: 300px;
	height: 30px;
	font-size: 20px;
	line-height: 30px;
}
.content_title {
	text-align: center;
	color: white;
	text-decoration: none;
}
.content_title a {
	color: white;
	text-decoration: none;
} 

.content_info li a{
    text-decoration: none;
		font-size:16px;  
}
.content_info  .content_info li a{
    color: #8b8b8b;
}
.content_info  .content_info li a:hover,.content_info  .content_info li a:active,.infoshow li a:hover,.infoshow li a:active,.infoshow_2 li a:hover,.infoshow_2 li a:active,.footer a:hover,.footer a:active{
    color: red;
}
.content_middle{
	width: 1200px;
}  
.content_middle_top_show{
    height: 500px;
    width: 900px;   
    float: left;
    background-color: rgba(60,60,60,0.8); /* 其他浏览器 */
}
.N1,.N2,.N3,.N4,.N5,.N6,.N7{
    height: 500px;
    width: 900px;  
    color: white;
}
.N1 ul,.N2 ul,.N3 ul,.N4 ul,.N5 ul,.N6 ul,.N7 ul{
    width: 900px;
    height: 65px;
    list-style-type: none;
}
.N1 p,.N2 p,.N3 p,.N4 p,.N5 p,.N6 p,.N7 p{
    height: 56px;
    line-height: 56px;
    float: left;
    font-size: 22px;
		color:#d0d0d0;
    padding-left: 35px;
}
.N1 li,.N2 li ,.N3 li ,.N4 li,.N5 li ,.N6 li,.N7 li{
    float: left;
    height: 56px;
    line-height: 56px;
    margin-left: 20px;
}
.N1 a,.N2 a ,.N3 a ,.N4 a,.N5 a ,.N6 a,.N7 a{
    text-decoration: none;
	color: white;
	font-weight:bold;
}
.N1 a:hover,.N2 a:hover ,.N3 a:hover ,.N4 a:hover,.N5 a:hover ,.N6 a:hover,.N7 a:hover,.N1 a:active,.N2 a:active ,.N3 a:active ,.N4 a:active,.N5 a:active ,.N6 a:active,.N7 a:active{
    color: red;
}
.N1 span,.N2 span ,.N3 span ,.N4 span,.N5 span ,.N6 span,.N7 span{
    margin-left: 15px;
}

/*----move----*/
#content .switch{
	width:1200px;
	height:59px;
	background:#dedede;
	border-bottom:1px solid #ff0000;
	font-size:16px;
	line-height:30px;
}
#content .switch li{
	display:inline-block;
}
#content .switch a{
	display:inline-block;
	padding:15px;
}
#content .switch a:hover,#content .switch .active{
	color:#fff !important;	
	background:#ff0000;
}
#content .others .title{
	height:60px;
	margin:30px 0;
	background:#a3a3a3;
	line-height:60px;
	text-align:center;
}
#content .brand_show{
	text-align:center;	
}
#content .detail_info ul{
	padding:0 100px;
}
#content .detail_info .details li{
	font-size:26px;
	line-height:60px;
}
#content .detail_info .details span{
	color:#6b6b6b;	
}
#content .purchase_process .process{
	width:1100px;
	height:200px;
	margin:80px 50px;
	background:url(../images/details/purchase_process_bg.jpg) repeat-x;
}
#content .purchase_process ul{
	padding-top:54px;	
}
#content .purchase_process ul li{
	margin-right:49px;
	display:inline-block;
	font-size:16px;
	line-height:30px;	
}
#content .purchase_process ul .fifth{
	margin-right:0;
}
#content .purchase_process ul span{
	color:#fff;
	font-size:18px;	
}
/*轮播图*/
#content .brand_recommand .carousel_figure{
	margin:80px 0;
}
#content .brand_recommand .carousel_figure a{
	margin:0 4px;	
}
#content .brand_recommand .carousel_figure .left,#content .brand_recommand .carousel_figure .right{
	margin-top:36px;	
}
/*下方*/
#footer{
	height:450px;
	background:#f5f5f5;
}
#footer .upper{
	width:1200px;
	height:120px;
	padding-top:60px;
	margin:0 auto;
}
#footer .upper li{
	float:left;
	width:250px;
	padding:0 25px;
	color:#9e9898;
	text-align:center;
}
#footer .upper span{
	font-size:40px;
	color:#000;
}
#footer .lower{
	width:1200px;
	height:269px;
	border-top:1px dashed #9e9898;
	margin:0 auto;
}
#footer .lower ul{
	width:200px;
	padding-top:70px;
	float:left;
	text-align:center;
}
#footer .lower p{
	font-size:26px;
}
#footer .lower a{
	font-size:18px;
	color:#9e9898;
}
#footer .lower .service{
	width:370px;
	float:right;
	padding:70px 0 0 30px;
	text-align:left;
}
#footer .service h1{
	font-size:26px;
	font-weight:normal;
}
#footer .service h2{
	font-size:26px;
	color:#d32020;
	float:left;
	border:none;
}
#footer .service h3{
	width:230px;
	height:34px;
	line-height:34px;
	display:inline-block;
	border:1px solid #444; 
}
#footer .service span{
	background:#444;
	color:#fff;
	padding:5px 20px;
}
/*底部*/
#botoom{
	height:150px;
	background:#444;
}
#botoom .relatives{
	padding-top:40px;
	text-align:center;
	color:#fff;
} 
#botoom .relatives a{
	padding:0 30px;
	color:#fff;
}